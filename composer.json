{
    "name": "gergoerdosi/renamer",
    "license": "MIT",
    "description": "Renamer is a batch file renaming tool that allows you to rename lots of files quickly and conveniently.",
    "require": {
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Renamer\\": "src/Renamer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Renamer\\Tests\\": "tests/Renamer/"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-text",
        "php-cs-fixer": "php-cs-fixer fix"
    }
}
